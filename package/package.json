{
	"name": "astro-typed-ids",
	"version": "0.1.2",
	"description": "An Astro integration to get typed ids for Content Layer entries, matching Astro 4 behavior.",
	"type": "module",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"prepublishOnly": "pnpm build"
	},
	"keywords": [
		"astro-integration",
		"astro-component",
		"withastro",
		"astro",
		"utility"
	],
	"author": "florian-lefebvre",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/florian-lefebvre/astro-typed-ids.git"
	},
	"bugs": "https://github.com/florian-lefebvre/astro-typed-ids/issues",
	"homepage": "https://github.com/florian-lefebvre/astro-typed-ids#readme",
	"license": "MIT",
	"peerDependencies": {
		"astro": "^5.0.0-beta.10"
	},
	"devDependencies": {
		"@types/node": "^22.9.0",
		"tsup": "^8.3.5"
	}
}
